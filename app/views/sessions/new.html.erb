<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, class: 'input_area' %>
  <%= f.label :password %>
  <%= f.password_field :password, class: 'input_area' %>
  <%= f.submit "Log in", class: 'button' %>
<% end %>
